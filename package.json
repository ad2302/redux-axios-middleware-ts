{
  "name": "redux-axios-middleware-ts",
  "version": "0.1.0",
  "license": "MIT",
  "author": "bung87",
  "main": "dist/redux-axios-middleware-ts.cjs.js",
  "module": "dist/redux-axios-middleware-ts.esm.js",
  "typings": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "rollpkg build",
    "eslint": "eslint src --ext .ts",
